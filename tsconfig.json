{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "allowJs": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2015.iterable",
      "es2016",
      "es2017",
      "es2017.object",
      "es2018"
    ],
    "typeRoots": ["node_modules/@types", "./client/@types/typings.d.ts"]
  },
  "include": [
    "src/**/*",
    "webpack.ts",
    "client/components/PlayerPool.tsx",
    "client/components/EditableLineup.tsx",
    "client/components/GamesBar.tsx",
    "client/components/PoolFilters.tsx",
    "optimizer/lib/getValues.ts",
    "client/redux/initialState.ts"
  ],
  "exclude": ["node_modules"]
}
