{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "es2015", "es2015.iterable", "es2018", "es2016", "es2017"],
    "typeRoots": ["node_modules/@types", "client/types"]
  },
  "include": [
    "src",
    "webpack.ts",
    "client/PoolFilters.tsx",
    "client/types/typings.d.ts",
    "client/types/react-app-env.d.ts",
    "client/react-app-env.d.ts"
  ],
  "exclude": ["node_modules"]
}
